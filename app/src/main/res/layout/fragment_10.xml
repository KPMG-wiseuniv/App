<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_background"
    android:visibility="visible"
    tools:context=".Fragment10">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:textColor="@color/white"
        android:text="Lastly, you're going to answer three questions!"
        android:textSize="25dp"
        android:textAlignment="textStart"
        android:gravity="start"></TextView>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="160dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:textColor="@color/white"
        android:text="Please do not think deeply and answer honestly"
        android:textSize="17dp"
        android:textAlignment="textStart"
        android:gravity="start"></TextView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:textColor="@color/white"
        android:text="(2/3)"
        android:textSize="17dp"
        android:textAlignment="center"
        android:gravity="center_horizontal"></TextView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginTop="270dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:orientation="horizontal">
    <TextView
        android:layout_width="190dp"
        android:layout_height="25dp"
        android:textColor="@color/white"
        android:text="Your preference is"
        android:textSize="22dp"
        android:textAlignment="textStart"
        android:gravity="start"></TextView>

    <TextView
        android:id="@+id/style_result"
        android:layout_width="180dp"
        android:layout_height="25dp"
        android:layout_marginRight="20dp"
        android:textColor="@color/white"
        android:text="result"
        android:textSize="22dp"
        android:textAlignment="textStart"
        android:gravity="start"></TextView>
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:textColor="@color/white"
        android:text="Then, Choose one more appropriate for you."
        android:textSize="22dp"
        android:textAlignment="textStart"
        android:gravity="start"></TextView>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="600dp"
        android:src="@drawable/logo_white"></ImageView>

</FrameLayout>